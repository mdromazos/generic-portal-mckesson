<xs:schema xmlns="urn:siperian.api" 
	xmlns:xs="http://www.w3.org/2001/XMLSchema" 
	xmlns:tns="urn:types.siperian.api" 
	xmlns:mdns="urn:metadata.siperian.api" 
	targetNamespace="urn:siperian.api" 
	elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:import namespace="urn:types.siperian.api" schemaLocation="sif-types.xsd"/>
	<xs:import namespace="urn:metadata.siperian.api" schemaLocation="sif-metadata.xsd"/>

	<xs:complexType name="SiperianRequestFault">
		<xs:sequence>
			<xs:element name="requestName" type="xs:string"/>
			<xs:element name="errorCode" type="xs:string"/>
			<xs:element name="errorMessage" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="SiperianRequestFault" type="SiperianRequestFault"/>
	<xs:complexType name="SiperianRequestException">
		<xs:sequence>
			<xs:element name="requestName" type="xs:string"/>
			<xs:element name="errorCode" type="xs:string"/>
			<xs:element name="errorMessage" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="SiperianRequestException" type="SiperianRequestException"/>
	
	<xs:complexType name="Password">
		<xs:sequence>
			<xs:element name="password" type="xs:string"/>
			<xs:element name="encrypted" type="xs:boolean" default="false"/>
		</xs:sequence>
	</xs:complexType>

    <xs:complexType name="AsynchronousOptions">
		<xs:sequence>
			<xs:element name="isAsynchronous" type="xs:boolean" default="false"/>
			<xs:element name="jmsReplyTo" type="xs:string" minOccurs="0"/>
			<xs:element name="jmsCorrelationId" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="SiperianRequest">
        <xs:annotation>
        	<xs:documentation>This is the base type for all requests.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="username" type="xs:string" nillable="true" minOccurs="0"/>
            <xs:element name="password" type="Password" nillable="true" minOccurs="0"/>
            <xs:element name="securityPayload" type="xs:hexBinary" nillable="true" minOccurs="0"/>
            <xs:element name="orsId" type="tns:OrsId" nillable="true" minOccurs="0"/>
						<xs:element name="interactionId" type="tns:InteractionId" nillable="true" minOccurs="0" />
			<xs:element name="asynchronousOptions" type="AsynchronousOptions" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>

    <xs:complexType name="SiperianObjectRequest">
		<xs:complexContent>
			<xs:extension base="SiperianRequest">
				<xs:sequence>
					<xs:element name="siperianObjectUid" type="tns:SiperianObjectUid"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="SiperianResponse">
        <xs:annotation>
        	<xs:documentation>This is the base type for all responses.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
			<xs:element name="message" type="xs:string" minOccurs="0" />
			<xs:element name="interactionId" type="tns:InteractionId"
				nillable="true" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="GBID">
        <xs:annotation>
        	<xs:documentation>This is a Global Business Identifier which can be used as a key for a data object. 'name' is the name of the collumn in the data object containing the GBID. 'value' is the GBID value.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
			<xs:element name="columnUid" type="tns:SiperianObjectUid"/>
			<xs:element name="value" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="RecordKey">
        <xs:annotation>
        	<xs:documentation>This is used to identify a record in a Siperian object (base object, ...). A combination of system name, rowid, source key, GBID can be used to identify a unique record.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
        	<xs:element name="systemName" type="tns:SystemName" minOccurs="0" />
        	<xs:element name="rowid" type="tns:Rowid" minOccurs="0" />
        	<xs:element name="sourceKey" type="tns:SourceKey" minOccurs="0" />
            <xs:element name="periodStartDate" type="xs:dateTime" maxOccurs="1" minOccurs="0"></xs:element>
            <xs:element name="periodEndDate" type="xs:dateTime" maxOccurs="1" minOccurs="0"></xs:element>
        	<xs:element name="GBID" type="GBID" minOccurs="0" maxOccurs="unbounded" />
            <xs:element name="rowidXref" type="tns:Rowid" maxOccurs="1" minOccurs="0"></xs:element>
        </xs:sequence>
	</xs:complexType>

	<xs:complexType name="XrefKey">
        <xs:annotation>
        	<xs:documentation>This is used to identify a record in a Siperian Xref object. A combination of system name and source key can be used to identify a unique record.</xs:documentation>
        </xs:annotation>
        	<xs:choice minOccurs="0" maxOccurs="1">
       	        <xs:sequence>
                    <xs:element name="systemName" type="tns:SystemName" />
                    <xs:element name="sourceKey" type="tns:SourceKey" />
                    <xs:element name="periodStartDate" type="xs:dateTime" maxOccurs="1" minOccurs="0"></xs:element>
                    <xs:element name="periodEndDate" type="xs:dateTime" maxOccurs="1" minOccurs="0"></xs:element>
                </xs:sequence>
       	        <xs:element name="rowidXref" type="tns:Rowid"></xs:element>
        	</xs:choice>
	</xs:complexType>

	<xs:complexType name="Parameter">
        <xs:annotation>
        	<xs:documentation>A single data parameter within a data record - has a value.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
 			<xs:choice minOccurs="0" maxOccurs="1">
				<xs:element name="stringValue" type="xs:string"></xs:element>
				<xs:element name="bigIntegerValue" type="xs:integer"></xs:element>
				<xs:element name="bigDecimalValue" type="xs:decimal"></xs:element>
				<xs:element name="dateValue" type="xs:dateTime"></xs:element>
				<xs:element name="booleanValue" type="xs:boolean"></xs:element>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="Field">
        <xs:annotation>
        	<xs:documentation>A single data field within a data record - has a name and a value.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
          <xs:extension base="Parameter">
	        <xs:sequence>
				<xs:element name="name" type="xs:string" />
			</xs:sequence>
		  </xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="TrustOverrideField">
            <xs:complexContent>
                  <xs:extension base="Field">
                        <xs:sequence>
                              <xs:element name="trustOverride" type="TrustSetting"></xs:element>
                        </xs:sequence>
                  </xs:extension>
            </xs:complexContent>
    </xs:complexType>
    
    <xs:complexType name="MatchPathField">
                <xs:complexContent>
                      <xs:extension base="Field">
                            <xs:sequence>
                                  <xs:element name="matchPathUID" type="xs:string"/>
                            </xs:sequence>
                      </xs:extension>
                </xs:complexContent>
    </xs:complexType>

	<xs:complexType name="TrustEnabledField">
            <xs:complexContent>
                  <xs:extension base="Field">
                        <xs:sequence>
                              <xs:element name="trustScore" type="xs:float" minOccurs="0"></xs:element>
                              <xs:element name="survivor" type="xs:boolean" minOccurs="0"></xs:element>
                              <xs:element name="trustSetting" type="TrustSetting" minOccurs="0"></xs:element>
                        </xs:sequence>
                  </xs:extension>
            </xs:complexContent>
    </xs:complexType>
    
	<xs:complexType name="BvtField">
            <xs:complexContent>
                  <xs:extension base="Field">
                        <xs:sequence>
                              <xs:element name="rowidLineage" type="tns:Rowid" minOccurs="0"></xs:element>
                              <xs:element name="sourceKey" type="xs:string" minOccurs="0"></xs:element>
                              <xs:element name="systemName" type="xs:string" minOccurs="0"></xs:element>
                        </xs:sequence>
                  </xs:extension>
            </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="RecordTypes">
      <xs:sequence>
        <xs:element name="package" type="xs:boolean" minOccurs="0" maxOccurs="1" default="true"/>
        <xs:element name="xref" type="xs:boolean" minOccurs="0" maxOccurs="1" default="false"/>
        <xs:element name="pendingXref" type="xs:boolean" minOccurs="0" maxOccurs="1" default="false"/>
        <xs:element name="deletedXref" type="xs:boolean" minOccurs="0" maxOccurs="1" default="false"/>
        <xs:element name="history" type="xs:boolean" minOccurs="0" maxOccurs="1" default="false"/>
        <xs:element name="xrefHistory" type="xs:boolean" minOccurs="0" maxOccurs="1" default="false"/>
        <xs:element name="raw" type="xs:boolean" minOccurs="0" maxOccurs="1" default="false"/>
      </xs:sequence>
    </xs:complexType>    

    <xs:simpleType name="MatchType">
      <xs:restriction base="xs:string">
        <xs:enumeration value="AUTO"/>
        <xs:enumeration value="BOTH"/>
        <xs:enumeration value="DBFILTERED"/>
        <xs:enumeration value="NONE"/>
      </xs:restriction>
    </xs:simpleType> 

    <xs:complexType name="Record">
        <xs:annotation>
            <xs:documentation>This is a single record of data.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:element name="field" type="Field"/>
                <xs:element name="trustOverrideField" type="TrustOverrideField"/>
                <xs:element name="trustEnabledField" type="TrustEnabledField"/>
                <xs:element name="matchPathField" type="MatchPathField"/>
                <xs:element name="bvtField" type="BvtField"/>
            </xs:choice>
            <xs:element name="siperianObjectUid" type="tns:SiperianObjectUid"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="HMRecord">
        <xs:complexContent>
            <xs:extension base="Record">
                <xs:sequence>
                	<xs:element name="recordKey" type="RecordKey" />
                	<xs:element name="aggregatePeriodStartDate" />
                	<xs:element name="aggregatePeriodEndDate" />
                	<xs:element name="notInEffect" type="xs:boolean" />
                	<xs:element name="lockedByInteractionId" type="xs:string"></xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:simpleType name="ActionType">
      <xs:restriction base="xs:string">
			<xs:enumeration value="Insert"/>
			<xs:enumeration value="Update"/>
			<xs:enumeration value="Accept as Unique"/>
			<xs:enumeration value="Merge"/>
			<xs:enumeration value="Delete"/>
			<xs:enumeration value="Update XREF"/>
			<xs:enumeration value="Merge Update"/>
			<xs:enumeration value="UnMerge"/>
			<xs:enumeration value="No Action"/>
			<xs:enumeration value="Pending Insert"/>
			<xs:enumeration value="Pending Update"/>
			<xs:enumeration value="Pending Update XREF"/>
			<xs:enumeration value="Promote Pending XREF"/>
			<xs:enumeration value="BO set to Delete"/>
			<xs:enumeration value="XREF set to Delete"/>
			<xs:enumeration value="BO Delete"/>
			<xs:enumeration value="XREF Delete"/>
			<xs:enumeration value="AM Rule Event"/>
      </xs:restriction>
    </xs:simpleType> 

    <xs:simpleType name="RecordState">
      <xs:restriction base="xs:string">
        <xs:enumeration value="CONSOLIDATED"/>
        <xs:enumeration value="UNMERGED"/>
        <xs:enumeration value="QUEUED_FOR_MATCH"/>
        <xs:enumeration value="NEWLY_LOADED"/>
        <xs:enumeration value="ON_HOLD"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="LookupField">
        <xs:annotation>
        	<xs:documentation>This is used to identify a column in a base object. uid can be use to identify the column and value is for look up.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
			<xs:element name="uid" type="tns:SiperianObjectUid" />
			<xs:element name="value" type="xs:string" />
		</xs:sequence>
	</xs:complexType>
	
	<xs:simpleType name="ComparisonOperator">
      <xs:restriction base="xs:string">
        <xs:enumeration value="EQUALS"/>
        <xs:enumeration value="NOT_EQUALS"/>
        <xs:enumeration value="GREATER_THAN"/>
        <xs:enumeration value="GREATER_THAN_OR_EQUAL"/>
        <xs:enumeration value="LESS_THAN"/>
        <xs:enumeration value="LESS_THAN_OR_EQUAL"/>
        <xs:enumeration value="LIKE"/>
        <xs:enumeration value="LIKE_ANYWHERE"/>
        <xs:enumeration value="LIKE_STARTS_WITH"/>
        <xs:enumeration value="LIKE_ENDS_WITH"/>
        <xs:enumeration value="NOT_LIKE"/>
        <xs:enumeration value="NOT_LIKE_ANYWHERE"/>
        <xs:enumeration value="NOT_LIKE_STARTS_WITH"/>
        <xs:enumeration value="NOT_LIKE_ENDS_WITH"/>
      </xs:restriction>
    </xs:simpleType>
    
    <xs:simpleType name="TrustTimeUnit">
      <xs:restriction base="xs:string">
        <xs:enumeration value="Day"/>
        <xs:enumeration value="Week"/>
        <xs:enumeration value="Month"/>
        <xs:enumeration value="Quarter"/>
        <xs:enumeration value="Year"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="TrustGraphType">
      <xs:restriction base="xs:string">
        <xs:enumeration value="Linear"/>
        <xs:enumeration value="Slow Initial, Rapid Later"/>
        <xs:enumeration value="Rapid Initial, Slow Later"/>
      </xs:restriction>
    </xs:simpleType>

	<xs:complexType name="TrustSetting">
        <xs:annotation>
        	<xs:documentation>This is used to describe trust settings for a column.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
			<xs:element name="custom" type="xs:boolean"/>
			<xs:element name="maximumTrust" type="xs:integer" minOccurs="0" maxOccurs="1"/>
			<xs:element name="minimumTrust" type="xs:integer" minOccurs="0" maxOccurs="1"/>
			<xs:element name="timeUnit" type="TrustTimeUnit" minOccurs="0" maxOccurs="1"/>
			<xs:element name="maximumTimeUnits" type="xs:integer" minOccurs="0" maxOccurs="1"/>
			<xs:element name="graphType" type="TrustGraphType" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="TrustGraphData">
        <xs:annotation>
        	<xs:documentation>This is used to describe trust settings for a column.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
			<xs:element name="date" type="xs:date" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="trustValue" type="xs:integer" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="MergeHistoryNode">
        <xs:annotation>
        	<xs:documentation>This is a node in the merge history tree of a BO record.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
			<xs:element name="rowid" type="tns:Rowid"/>
			<xs:element name="mergeDate" type="xs:dateTime"/>
			<xs:element name="historyNode" type="MergeHistoryNode" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="SiperianObjectRecordKey">
        <xs:annotation>
        	<xs:documentation>This is a recordKey plus a siperianObjectUid to identify the entity type.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
			<xs:element name="recordKey" type="RecordKey"/>
			<xs:element name="siperianObjectUid" type="tns:SiperianObjectUid"/>
		</xs:sequence>
	</xs:complexType>

    <xs:complexType name="OneHopFilter">
        <xs:annotation>
        	<xs:documentation>This is used to specifies filtering criteria.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
			<xs:element name="hierarchies" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="relTypes" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="relDirections" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="activeRelsOnly" type="xs:boolean" default="true"/>            			
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="EntityGraphFilter">
		<xs:complexContent>
			<xs:extension base="OneHopFilter">
				<xs:sequence>
					<xs:element name="maximumDepth" type="xs:integer"/>
					<xs:element name="maximumBreadth" type="xs:integer"/>
					<xs:element name="maximumRelationships" type="xs:integer"/>
					<xs:element name="countLeafRelations" type="xs:boolean" default="true" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
    <xs:complexType name="EntityFilterCriteria">
        <xs:annotation>
        	<xs:documentation>This is used to filter entities by data values.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
			<xs:element name="entityTypeUid" type="xs:string" minOccurs="1" maxOccurs="1"/>
			<xs:element name="filterCriteria" type="xs:string" minOccurs="1" maxOccurs="1"/>
			<xs:element name="filterParameter" type="Parameter" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="EntityInfo">
        <xs:annotation>
        	<xs:documentation>This is used to specifies an entity infomation.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="siperianObjectRecordKey" type="SiperianObjectRecordKey" />
			<xs:element name="relationshipCount" type="xs:integer" />
			<xs:element name="traversed" type="xs:boolean" />
			<xs:element name="listNode" type="xs:boolean" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="XrefResult">
        <xs:annotation>
        	<xs:documentation>This is used to provide success/failure information on individual xrefs.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
			<xs:element name="xrefKey" type="XrefKey"/>
			<xs:element name="success" type="xs:boolean"/>
			<xs:element name="message" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="RecordResult">
        <xs:annotation>
        	<xs:documentation>This is used to provide success/failure information on individual records.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
			<xs:element name="recordKey" type="RecordKey"/>
			<xs:element name="success" type="xs:boolean"/>
			<xs:element name="message" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>

    <xs:simpleType name="HubState">
      <xs:restriction base="xs:string">
        <xs:enumeration value="ACTIVE"/>
        <xs:enumeration value="PENDING"/>
        <xs:enumeration value="DELETED"/>
      </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="AuthenticateRequest">
		<xs:complexContent>
			<xs:extension base="SiperianRequest"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="authenticate" type="AuthenticateRequest"/>
	<xs:complexType name="AuthenticateResponse">
		<xs:complexContent>
			<xs:extension base="SiperianResponse">
				<xs:sequence>
					<xs:element name="roleName" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="passwordExpires" type="xs:boolean" nillable="false"/>
					<xs:element name="passwordExpirationDate" type="xs:dateTime" nillable="true" minOccurs="0"/>
					<xs:element name="externalAuthentication" type="xs:boolean" nillable="false"/>
					<xs:element name="administrator" type="xs:boolean" nillable="false"/>
					<xs:element name="username" type="xs:string" nillable="true" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="authenticateReturn" type="AuthenticateResponse"/>

    	<xs:complexType name="GetOrsListRequest">
		<xs:complexContent>
			<xs:extension base="SiperianRequest"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="getOrsList" type="GetOrsListRequest"/>
	<xs:complexType name="GetOrsListResponse">
		<xs:complexContent>
			<xs:extension base="SiperianResponse">
				<xs:sequence>
					<xs:element name="ors" type="mdns:MetaDataOrs" minOccurs="0" maxOccurs="unbounded"/>					
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="getOrsListReturn" type="GetOrsListResponse"/>

	<xs:complexType name="GetRequest">
		<xs:complexContent>
			<xs:extension base="SiperianObjectRequest">
				<xs:sequence>
					<xs:element name="recordKey" type="RecordKey" />
					<xs:element name="systemName" type="tns:SystemName"
						minOccurs="0" maxOccurs="unbounded" />
					<xs:element name="recordTypes" type="RecordTypes"
						minOccurs="0" maxOccurs="1" />
					<xs:element name="returnTrustScores"
						type="xs:boolean" minOccurs="0" maxOccurs="1" default="false" />
					<xs:element name="returnLineage" type="xs:boolean"
						minOccurs="0" maxOccurs="1" default="false" />
					<xs:element name="effectiveDate" type="xs:dateTime"
						minOccurs="0" maxOccurs="1" />
					<xs:element name="historyDate" type="xs:dateTime"
						minOccurs="0" maxOccurs="1" />
					<xs:element name="dataFilter" type="xs:string" minOccurs="0" maxOccurs="1"></xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="get" type="GetRequest"/>
	<xs:complexType name="GetResponse">
		<xs:complexContent>
			<xs:extension base="SiperianResponse">
				<xs:sequence>
					<xs:element name="recordKey" type="RecordKey"/>
			        <xs:element name="record" type="Record" minOccurs="0" maxOccurs="unbounded"/>
			        <xs:element name="lastUpdateDate" type="xs:dateTime" minOccurs="0" maxOccurs="1"/>
			        <xs:element name="lastUpdatingXref" type="XrefKey" minOccurs="0" maxOccurs="1"/>
			        <xs:element name="aggregatePeriodStartDate" type="xs:dateTime" minOccurs="0" maxOccurs="1"/>
			        <xs:element name="aggregatePeriodEndDate" type="xs:dateTime" minOccurs="0" maxOccurs="1"/>
			        <xs:element name="lockedByInteractionId" type="xs:boolean" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="getReturn" type="GetResponse"/>

	<xs:complexType name="GetXrefForEffectiveDateRequest">
		<xs:complexContent>
			<xs:extension base="SiperianObjectRequest">
				<xs:sequence>
					<xs:element name="recordKey" type="RecordKey"/>
			        <xs:element name="systemName" type="tns:SystemName" minOccurs="0" maxOccurs="unbounded"/>
			        <xs:element name="recordState" type="HubState" minOccurs="0" maxOccurs="3"></xs:element>					<xs:element name="effectiveDate" type="xs:dateTime" minOccurs="0" maxOccurs="1"/>
			        <xs:element name="historyDate" type="xs:dateTime" minOccurs="0" maxOccurs="1"/>
			        <xs:element name="dataFilter" type="xs:string" minOccurs="0" maxOccurs="1"></xs:element>
			        <xs:element name="otherPeriods" type="xs:boolean" default="false" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="getXrefForEffectiveDate" type="GetXrefForEffectiveDateRequest"/>
	
	<xs:complexType name="GetXrefForEffectiveDateResponse">
		<xs:complexContent>
			<xs:extension base="SiperianResponse">
				<xs:sequence>
					<xs:element name="recordKey" type="RecordKey"/>
			        <xs:element name="record" type="Record" minOccurs="0" maxOccurs="unbounded"/>

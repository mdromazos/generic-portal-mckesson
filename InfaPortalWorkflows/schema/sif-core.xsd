	<xs:complexType name="SearchQueryRequest">
		<xs:complexContent>
			<xs:extension base="SearchRequestBase">
				<xs:sequence>
					<xs:element name="filterCriteria" type="xs:string">
						<xs:annotation>
							<xs:documentation>
								A parameterized condition clause. Ex. FIRST_NAME = ?
							</xs:documentation>
						</xs:annotation></xs:element>
					<xs:element name="filterParameter" type="Parameter" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>
								A list list of parameter values for binding.
							</xs:documentation>
						</xs:annotation></xs:element>
					<xs:element minOccurs="0" name="disablePaging" type="xs:boolean"/>
					<xs:element name="recordState" type="HubState" minOccurs="0" maxOccurs="3">
						<xs:annotation>
							<xs:documentation>
								Determines which hub states to include in the search. If not specified, then all states are included.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="joinUid" type="xs:string" minOccurs="0" maxOccurs="3">
						<xs:annotation>
							<xs:documentation>
								Determines which uids to include in the search results. If not specified, then only the siperian object uid is used.
							</xs:documentation>
						</xs:annotation>
					</xs:element>					
					<xs:element minOccurs="0" name="removeDuplicates" type="xs:boolean" default="false">
						<xs:annotation>
							<xs:documentation>
     						Enable or disable de-duplication of the result set. Defaults to false.
     						Enabling this option can degrade the performance of this request. It should
     						be enabled only when there is a possibility of duplicates in the result set
     						due to the structure of the query. E.g. - a query for a party based on attributes
     						of its child address record could result in the same party being returned
     						several time.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs="0" maxOccurs="1" name="advancedMode" type="xs:boolean" default="false">
						<xs:annotation>
							<xs:documentation>
     						Enable or disable special mode of search query processing. Defaults to false.
						When this mode is enabled, advanced operators in filterCriteria are supported.
						Currently only EXISTS and COUNT can be used. Please notice, that these operators
						do not use exact SQL syntax, but instead provide their own.
						Valid examples:
						COUNT(MT_ADDRESS) > 0
						COUNT(MT_ADDRESS, MATCH_PATH_COMPONENT.MT_ADDRESS|STATE_CD = 'CA') > 0
						EXISTS(MT_ADDRESS, MATCH_PATH_COMPONENT.MT_ADDRESS|STATE_CD = 'CA')

							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="effectiveDate" type="xs:dateTime" minOccurs="0" maxOccurs="1" />
					<xs:element name="historyDate" type="xs:dateTime" minOccurs="0" maxOccurs="1" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
